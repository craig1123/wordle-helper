{
  "name": "wordle-solver",
  "description": "An Alogrithm that thinks for you to help you solve wordle",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "rimraf dist && parcel ./src/index.html",
    "prebuild": "rimraf dist",
    "build": "parcel build ./src/index.html --public-url ./ --dist-dir docs",
    "push": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.2.1",
    "@parcel/transformer-webmanifest": "^2.2.1",
    "parcel": "^2.2.1",
    "push-dir": "^0.4.1",
    "rimraf": "^3.0.2"
  }
}
